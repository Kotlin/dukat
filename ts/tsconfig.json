{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es2015"],
    "outDir": "./build/ts",
    "strict": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "listFiles": true,
    "listEmittedFiles": true,
    "baseUrl": "./",
    "noImplicitAny": false,
    "paths": {
      "typescript-services-api": ["./build/package/node_modules/typescript/lib/typescriptServices.js"]
    }
  },  
  "include": [
    "src/**/*.ts"
  ]
}